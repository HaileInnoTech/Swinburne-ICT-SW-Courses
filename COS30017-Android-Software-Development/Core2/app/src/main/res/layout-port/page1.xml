<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/SCREEN"
    tools:ignore="MissingDefaultResource">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="411dp"
        android:layout_height="114dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:layout_width="413dp"
        android:layout_height="647dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <LinearLayout
            android:layout_width="205dp"
            android:layout_height="626dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginEnd="20dp"
                android:id="@+id/imageView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/da_lat"
                android:contentDescription="@string/app_name"
                android:visibility="visible"/>

            <TextView
                android:id="@+id/vdalat"
                style="@style/TEXT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dalat"
                android:gravity="center" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="199dp">

                <TextView
                    android:id="@+id/rate_dalat"
                    android:layout_width="65dp"
                    android:layout_height="23dp"
                    android:text="TextView"
                    style="@style/RATE"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:layout_marginEnd="20dp"

                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="159dp"
                android:src="@drawable/ho_tri_an"
               />

            <TextView
                android:id="@+id/vtrian"
                style="@style/TEXT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/trian"
                android:gravity="center" />

            <TextView
                android:id="@+id/rate_trian"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                style="@style/RATE"

                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="206dp"
            android:layout_height="626dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginStart="20dp"
                android:id="@+id/imageView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/doi_con_heo"
                 />

            <TextView
                android:id="@+id/vpighill"
                style="@style/TEXT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/conheo"
                android:gravity="center" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="201dp">

                <TextView
                    style="@style/RATE"

                    android:id="@+id/rate_pighill"
                    android:layout_width="65dp"
                    android:layout_height="26dp"
                    android:text="TextView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:layout_marginStart="20dp"

                android:id="@+id/imageView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/landmark_81"
                />

            <TextView
                android:id="@+id/vlandmark"
                style="@style/TEXT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/landmark"
                android:gravity="center" />

            <TextView
                android:id="@+id/rate_landmark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                style="@style/RATE"

                android:gravity ="center" />

        </LinearLayout>

    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="412dp"
        android:layout_height="39dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>